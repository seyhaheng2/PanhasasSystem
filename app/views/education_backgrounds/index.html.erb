


<div class="row">
  <div class="col-md-12">
    <div class="widget-area">

     <div class="streaming-table">

      <span style="display: none;" id="found" class="label label-info"></span>
      <div class="heading">
        <strong>Listing</strong> education_backgrounds
        <input name="search" id="st_search" class="st_search" placeholder="Search Here" type="text">
      </div>
      <div class="table-responsive">
        <table id="stream_table" class="table table-striped table-bordered table-hover">
          <thead>
            <tr>
                            <th>High school</th>
                            <th>English program</th>
                            <th>Degree program</th>
                            <th>University</th>
                            <th>Graduate school</th>
                            <th>Type of degree</th>
                            <th>Desired major</th>
                            <th>Student</th>
                            <th></th>
            </tr>
          </thead>

          <tbody>
            <%= content_tag_for(:tr, @education_backgrounds) do |education_background| %>
                        <td><%= education_background.high_school %></td>
                        <td><%= education_background.english_program %></td>
                        <td><%= education_background.degree_program %></td>
                        <td><%= education_background.university %></td>
                        <td><%= education_background.graduate_school %></td>
                        <td><%= education_background.type_of_degree %></td>
                        <td><%= education_background.desired_major %></td>
                        <td><%= education_background.student %></td>
                        <td><%= link_to '<i class="fa fa-book"></i>'.html_safe, education_background, class: "btn btn-sm btn-default" %>
            <%= link_to '<i class="fa fa-pencil-square-o"></i>'.html_safe, edit_education_background_path(education_background), class: "btn btn-sm btn-primary" %>
            <%= link_to '<i class="fa fa-remove (alias)"></i>'.html_safe, education_background, class: "btn btn-sm btn-danger", method: :delete, data: { confirm: 'Are you sure?' } %></td>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>  
    <div class="">
     <%= link_to new_education_background_path, :class => 'btn btn-primary' do %>
        <span class="fa fa-plus"></span>
        New 
        <% end %>
    </div>
  </div></div>
</div>